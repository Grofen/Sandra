<!DOCTYPE html>
<html>
	<head>
		<title>{{websiteTitle}}</title>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<link rel="icon" href="images/logo1.png">

		<!-- Stylesheets -->
		{{> stylesheets}}
	</head>

	<body>
		<!-- Navbar -->
		{{> header}}

		<!-- Features Section -->
		<a href="/shop"><section class="features-section background" style="background-image: url('images/New-Banner-130.jpg')"></section></a>

		<!-- Shop New Arivals -->
		<div class="container-fluid narivals-section">
			<div class="row">
				<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 item">
					<a href="/shop">
						<div class="arivals-img background" style="background-image: url('images/I.AM.GIA-4.jpg');"></div>
					</a>
				</div>
				<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 item">
					<a href="/shop">
						<div class="arivals-img background" style="background-image: url('images/Playsuits-31.jpg');"></div>
					</a>
				</div>
			</div>
		</div>

		<!-- Shop items -->
		<div class="container-fluid shopHomeItems">
			<div class="row">
				<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 item">
					<a href="/shop">
						<div class="shop-img background" style="background-image: url('images/Shoes-42.jpg');"></div>
					</a>
				</div>
				<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 item">
					<a href="/shop">
						<div class="shop-img background" style="background-image: url('images/JeffreeStar-47.jpg');"></div>
					</a>
				</div>
				<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 item">
					<a href="/shop">
						<div class="shop-img background" style="background-image: url('images/Beauty-37.jpg');"></div>
					</a>
				</div>
			</div>
		</div>

		<!-- Instashop -->
		<section class="instashopSection">
			<div class="container-fluid">
				<div class="row">
					<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 instaHeader">
						<img src="images/instagram-text.1512022547.png" width="220" height="44" alt="">
					</div>
					<div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
						<div class="instaImg background" style="background-image: url('images/Playsuits-32.jpg')"></div>
					</div>
					<div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
						<div class="instaImg background" style="background-image: url('images/1_70_145.1512022547.jpg')"></div>
					</div>
					<div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
						<div class="instaImg background" style="background-image: url('images/1_85_57.1512022547.jpg')"></div>
					</div>
					<div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
						<div class="instaImg background" style="background-image: url('images/2_73_234.1512022547.jpg')"></div>
					</div>
					<div class="col-md-12 text-center instalink">
						<p><a href="https://www.instagram.com/?hl=en"><i class="fa fa-instagram"></i> @Sandra</a></p>
					</div>
				</div>
			</div>
		</section>

		<!-- Footer -->
		{{> footer}}

		<!-- Scripts
		============================== -->
		{{> scripts}}
	</body>
</html>
